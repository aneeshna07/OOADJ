<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Controller.notes.ViewImageController">
    <children>
        <ImageView fx:id="imageView" fitHeight="300.0" fitWidth="400.0" layoutX="22.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="prevButton" layoutX="115.0" layoutY="324.0" mnemonicParsing="false" onAction="#onPrevButtonClick" text="&lt;">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font></Button>
        <Button fx:id="nextButton" layoutX="275.0" layoutY="324.0" mnemonicParsing="false" onAction="#onNextButtonClick" text="&gt;">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font></Button>
    </children>
</AnchorPane>
